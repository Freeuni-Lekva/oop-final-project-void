<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>TakeQuizServlet</servlet-name>
    <servlet-class>quiz.TakeQuiz.servlets.TakeQuizServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>StartQuizServlet</servlet-name>
    <servlet-class>quiz.TakeQuiz.servlets.StartQuizServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TakeQuizData</servlet-name>
    <servlet-class>quiz.TakeQuiz.servlets.TakeQuizData</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>SubmitQuizServlet</servlet-name>
    <servlet-class>quiz.submitQuiz.servlets.SubmitQuizServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TakeQuizServlet</servlet-name>
    <url-pattern>/quiz/take</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>StartQuizServlet</servlet-name>
    <url-pattern>/quiz/start</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TakeQuizData</servlet-name>
    <url-pattern>/quiz/take/data</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SubmitQuizServlet</servlet-name>
    <url-pattern>/quiz/submit</url-pattern>
  </servlet-mapping>

  <!-- Must come after servlet(-mapping) declarations -->
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
